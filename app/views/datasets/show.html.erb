<h1><%= @dataset.fullname %></h1>

<div id="holder" style="width:800px;height:500px">

<script>
	var r = Raphael('holder');
	chart = r.linechart(15, 0, 780, 490, <%= @data_x %>, [<%= @data_y %>, [300,400]], {axis: "0 0 1 1", colors: ["#FF0000", "transparent"]});
	var axisItems = chart.axis[0].text.items
    for( var i = 0, l = axisItems.length; i < l; i++ ) {
       var date = new Date(parseInt(axisItems[i].attr("text"))*1000);
       // using the excellent dateFormat code from Steve Levithan
       axisItems[i].attr("text", dateFormat(date, "mm/yyyy"));
    }
</script>